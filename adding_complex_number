// complete it using struct 
#include <stdio.h>
#include <complex.h>


int main() {
    double complex real_number;
    double complex complex_number;
    printf("Enter real number and complex nmumber: ");
    scanf("%cf %cf", &real_number, &complex_number);
    double complex result=creal(real_number)+cimag(complex_number);
    printf("%cf", result);

    return 0;
}

//Used by programizzz can refer this one 
#include <stdio.h>

typedef struct Complex{
    double real;
    double imaginary;
}complex;

int main() {
    complex c1 = {.real=24, .imaginary=2.8547};  
    complex c2=  {.real=14, .imaginary=58.149};
    complex sum;
    sum.real=c1.real+c2.real;
    sum.imaginary=c1.imaginary+c2.imaginary;
    printf("Sum is %.2lf + %.2lfi:", sum.real, sum.imaginary);
    return 0;
}
