//Array Basic 
#include <stdio.h>

int main() {
    int m;
    printf("Enter the No. of subjects: ");
    scanf("%d", &m);
    int averageMarks[m];
    printf("Enter input values: ");
    for(int i=0;i<m;++i){
        scanf("%d", &averageMarks[i]);
    }
    int add;
    add=averageMarks[0]+averageMarks[1]+averageMarks[2]+averageMarks[3]+averageMarks[4];  // Need to implement a loop to add the average number and divide it by the number of subjects 
    double result=add/m;
    printf("%lf", result);
    return 0;
}

// Updated With in 10 min 
#include <stdio.h>

int main() {
    int m;
    printf("Enter the No. of subjects: ");
    scanf("%d", &m);
    int averageMarks[m];
    printf("Enter input values: ");
    for(int i=0;i<m;++i){
        scanf("%d", &averageMarks[i]);
    }
    int add=0;
    for(int i=0;i<m;++i){
        add=add+averageMarks[i]; // Adding of the elements done 
    }
    double result=add/m;
    printf("%lf", result);
    return 0;
}
